	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int number,top;
		int total = 1;
		System.out.println("Üssü alınacak sayıyı giriniz");
		number = scan.nextInt();
		System.out.println("Üssü giriniz");
		top = scan.nextInt();
		
		int i = 1;
		while(i <= top) {
			
			total *= number;
			i++;
			
		}
		System.out.println(total);
		
	}
}
